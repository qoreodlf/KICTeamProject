<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="workbook">

	<insert id="addworkbook" parameterType="workbook">
		insert into workbook values 
		(#{userId}, wbseq.nextval, #{wbJmcd}, #{wbTittle}, #{wbText}, #{wbAnswer}, sysdate,0,0) 
	</insert>
	
	<select id="selectwblist" parameterType="string" resultType="workbook">
		select * from workbook where wbJmcd = #{value}
	</select>
	
	<select id="selectwbone" parameterType="string" resultType="workbook">
		select * from workbook where wbNum = #{value}
	</select>
</mapper>